<div class="well">
    <table class="table table-responsive">
        <thead>
        <tr>
            <th id="th-checkbox" width="1%">
                <div class="checkbox">
                    <label>
                        <input type="checkbox" onclick="window.click_checkbox_all('youtube_account', this.checked)">
                    </label>
                </div>
            </th>
            <th id="th-icons" width="1%">&nbsp;</th>
            <th id="th-account-name" width="48%">{% trans %}Name{% endtrans %}</th>
            <th id="th-account-login" width="40%">{% trans %}Login{% endtrans %}</th>
            <th id="th-multimedia-objects" width="10%">{% trans %}Multimedia objects{% endtrans %}</th>
        </tr>
        </thead>
        <tbody>
            {% for account in youtubeAccounts %}
                <tr class="active">
                    <td headers="th-checkbox" width="1%">
                        <div class="checkbox">
                            <label>
                                <input type="checkbox" class="youtube_account_checkbox" name="youtube_account_checkbox[]" value="{{ account.getId() }}">
                            </label>
                        </div>
                    </td>
                    <td headers="th-icons" width="1%">
                        {% include 'PumukitYoutubeBundle:Admin:updatemenu.html.twig' %}
                    </td>
                    <td headers="th-account-name" width="48%">
                        {{ account.getName() }}
                    </td>
                    <td headers="th-account-login" width="40%">{{ account.getLogin() }}</td>
                    <td headers="th-multimedia-objects" width="10%">0</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
</div>